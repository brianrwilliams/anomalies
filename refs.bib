@article{MS1,
	abstract = {We consider ten-dimensional supersymmetric Yang--Mills theory (10D SUSY YM theory) and its dimensional reductions, in particular, BFSS and IKKT models. We formulate these theories using algebraic techniques based on application of differential graded Lie algebras and associative algebras as well as of more general objects, Lâˆž- and Aâˆž-algebras. We show that using pure spinor formulation of 10D SUSY YM theory equations of motion and isotwistor formalism one can interpret these equations as Maurer--Cartan equations for some differential Lie algebra. This statement can be used to write BV action functional of 10D SUSY YM theory in Chern--Simons form. The differential Lie algebra we constructed is closely related to differential associative algebra (Î©,âˆ‚Ì„) of (0,k)-forms on some supermanifold; the Lie algebra is tensor product of (Î©,âˆ‚Ì„) and matrix algebra. We construct several other algebras that are quasiisomorphic to (Î©,âˆ‚Ì„) and, therefore, also can be used to give BV formulation of 10D SUSY YM theory and its reductions. In particular, (Î©,âˆ‚Ì„) is quasiisomorphic to the algebra (B,d), constructed by Berkovits. The algebras (Î©0,âˆ‚Ì„) and (B0,d) obtained from (Î©,âˆ‚Ì„) and (B,d) by means of reduction to a point can be used to give a BV-formulation of IKKT model. We introduce associative algebra SYM as algebra where relations are defined as equations of motion of IKKT model and show that Koszul dual to the algebra (B0,d) is quasiisomorphic to SYM.},
	author = {M. Movshev and A. Schwarz},
	doi = {https://doi.org/10.1016/j.nuclphysb.2003.12.033},
	issn = {0550-3213},
	journal = {Nuclear Physics B},
	number = {3},
	pages = {324-350},
	title = {On maximally supersymmetric Yang--Mills theories},
	url = {https://www.sciencedirect.com/science/article/pii/S0550321303011106},
	volume = {681},
	year = {2004},
